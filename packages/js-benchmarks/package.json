{
  "name": "js-benchmarks",
  "version": "0.0.0",
  "description": "",
  "author": "damiankoper <kopernickk@gmail.com>",
  "homepage": "https://github.com/damiankoper/js-acceleration#readme",
  "license": "ISC",
  "main": "dist/main.mjs",
  "types": "dist/main.d.ts",
  "type": "module",
  "module": "dist/main.mjs",
  "directories": {
    "src": "src"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/damiankoper/js-acceleration.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "bench:node": "node dist/js-sequential.node.mjs",
    "bench:deno": "deno run --allow-read --allow-write --allow-net src/js-sequential/js-sequential.deno.ts",
    "bench:chrome": "google-chrome --incognito --user-data-dir=/tmp --enable-benchmarking http://localhost:8080/js-sequential.web.html & npx -y http-server dist",
    "bench:firefox": "firefox --private-window http://localhost:8080/js-sequential.web.html & npx -y http-server dist",
    "bench:auto": "npm run bench:node && npm run bench:deno"
  },
  "bugs": {
    "url": "https://github.com/damiankoper/js-acceleration/issues"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.5",
    "@types/lodash": "^4.14.178",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "@webpack-cli/generators": "^2.4.1",
    "benchmark": "*",
    "eslint": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "fast-csv": "^4.3.6",
    "file-saver": "^2.0.5",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "html-webpack-plugin": "^5.5.0",
    "jimp": "^0.16.1",
    "js-sequential": "*",
    "lodash": "^4.17.21",
    "meta": "*",
    "platform": "^1.3.6",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "@types/papaparse": "^5.3.1",
    "papaparse": "^5.3.1"
  }
}